{
  "version": 2,
  "projects": [
    {
      "name": "my-nextjs-app",
      "root": "apps/nextjs",
      "buildCommand": "npm run build",
      "outputDirectory": "out"
    },
    {
      "name": "my-docusaurus-app",
      "root": "apps/docusaurus",
      "buildCommand": "npm run build",
      "outputDirectory": "build"
    }
  ],
  "rewrites": [
    {
      "source": "/docs/(.*)",
      "destination": "/apps/docusaurus/build/$1"
    }
  ]
}
